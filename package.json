    "react": "^18.2.0",
    "react-blockies": "^1.4.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.3",
    "react-hotjar": "^5.4.1",
    "react-redux": "^8.1.1",
    "react-toastify": "^9.1.3",
    "react-use": "^17.4.0",
    "react-use-intercom": "^5.1.4",
    "redux-persist": "^6.0.0",
    "serialize-error": "^8.1.0",
